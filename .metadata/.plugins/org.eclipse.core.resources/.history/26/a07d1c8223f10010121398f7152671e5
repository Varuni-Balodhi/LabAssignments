
package JavaBasics;
import java.util.Arrays;
public class Ques1 {
  public static void main(String[] args) {
	  int a=0;
	  int b=1;
	  int[] fib=Fibonacci(a,b);
	  System.out.println(Arrays.toString(fib));
  }
  public static int[] Fibonacci(int a,int b) {
	  int[] arr=new int[21];
	  arr[0]=0;
	  arr[1]=1;
	  for(int i=2;i<21;i++) {
		  arr[i]=arr[i-1]+arr[i-2];
	  }
	  return arr;
  }
}

